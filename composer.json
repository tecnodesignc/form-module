{
    "name": "tecnodesignc/form-module",
    "type": "encore-module",
    "description": "Form module for EncoreCMS 3",
    "keywords": ["forms","emails","Send Emails"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marcos Rativa",
            "email": "soporte@tecnodesign.com.co"
        }
    ],
    "support": {
        "email": "soporte@tecnodesign.com.co",
        "issues": "https://github.com/tecnodesignc/form-module/issues",
        "source": "https://github.com/tecnodesignc/form-module"
    },
    "require": {
        "php": ">=8.2",
        "composer/installers": "^2.1",
        "tecnodesignc/core-module": "^3.0",
        "josiasmontag/laravel-recaptchav3": "^1.0",
        "guzzlehttp/psr7": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "3.6.*"
    },
    "autoload-dev": {
        "psr-4": {
            "Modules\\Form\\": ".",
            "Modules\\": "modules/"
        }
    },
    "autoload": {
        "psr-4": {
            "Modules\\Form\\": ""
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,

    "extra": {
        "laravel": {
            "providers": [
                "Modules\\Form\\Providers\\FormServiceProvider"
            ],
            "aliases": {}
        }
    }

}
